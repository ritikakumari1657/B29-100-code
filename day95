Q145: Return a structure containing top student's details from a function.

#include <stdio.h>
#include <string.h>
    int roll_no;
    float marks;
    char name[50];
};
struct StudentDetails get_top_student_details() {
    struct StudentDetails topStudent;
    topStudent.roll_no = 101;
    topStudent.marks = 98.5;
    strcpy(topStudent.name, "Alice Smith");
    return topStudent;
}
int main() {
    struct StudentDetails student;
    student = get_top_student_details();
    printf("Top Student Details:\n");
    printf("Name: %s\n", student.name);
    printf("Roll No: %d\n", student.roll_no);
    printf("Marks: %.2f\n", student.marks);
    return 0;
}
