Q148: Take two structs as input and check if they are identical.

#include <stdio.h>
#include <string.h> 
typedef struct {
    int id;
    char name[50];
} Person;
int areStructsEqual(Person p1, Person p2) {
    if (p1.id != p2.id) {
        return 0;
    }
    if (strcmp(p1.name, p2.name) != 0) {
        return 0; 
    }
    return 1;
}
int main() {
    Person person1 = {1, "Alice"};
    Person person2 = {1, "Alice"};
    Person person3 = {2, "Bob"};
    if (areStructsEqual(person1, person2)) {
        printf("person1 and person2 are identical.\n");
    } else {
        printf("person1 and person2 are not identical.\n");
    }
    if (areStructsEqual(person1, person3)) {
        printf("person1 and person3 are identical.\n");
    } else {
        printf("person1 and person3 are not identical.\n");
    }
    return 0;
}
